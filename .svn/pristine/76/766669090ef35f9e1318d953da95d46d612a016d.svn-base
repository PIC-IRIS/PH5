#!/usr/bin/env bash
#   Version: 2014.105
EXEC='fix_das_chan_num'
MOD='pn3'

export LD_LIBRARY_PATH=${K3}/lib

h=$@

if [ ! -z "${K3}" ]
then
	PYTHON=pnpython3
	export PYTHON
        PYTHONPATH=${K3}/apps/${MOD}
        export PYTHONPATH
else
        echo "K3 environment not set! Can not continue."
	exit
fi

if [ -z "$PYTHON" ]
then
	echo "$PYTHON not found!"
	exit
fi

${PYTHON} ${K3}/apps/${MOD}/${EXEC}.py $h
